<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp1.0</TargetFramework>
    <Description>It is an extenion for mapping data to your model that returned from efcore stored procedure executed.</Description>
    <Authors>hadi eskandari</Authors>
    <PackageTags>efcore,storedprocedure,mapping,reflection,automapper</PackageTags>
    <RepositoryUrl>https://github.com/mhadieskandari/SqlServerExecuteStoredProcedureMapper</RepositoryUrl>
    <PackageProjectUrl>https://github.com/mhadieskandari/SqlServerExecuteStoredProcedureMapper</PackageProjectUrl>
    <Version>1.0.4</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
